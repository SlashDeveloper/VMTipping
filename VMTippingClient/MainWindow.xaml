<Window x:Class="VMTippingClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Command="{Binding ReadFilesCommand}" Width="150" Height="60" Margin="24" Content="Les filer" />
            <Button Command="{Binding SaveCommand}" Width="150" Height="60" Margin="24" Content="Lagre resultat" />
        </StackPanel>
    </Grid>
</Window>
